{
  "name": "unochat",
  "author": {
    "name": "Lucas Klauck",
    "email": "lucasklauck2@gmail.com"
  },
  "version": "1.0.5",
  "main": "src/app.js",
  "scripts": {
    "ng": "ng",
    "start": "ng build --base-href ./ && electron .",
    "build": "ng build",
    "package": "npx electron-forge package",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "pack": "electron-builder --dir",
    "dist": "ng build --base-href ./ && electron-builder",
    "electron-build": "ng build --prod && electron .",
    "electron-package": "electron-packager . FarmhubCompanyDashboard --platform=win32 --arch=x64",
    "publish": "npm run dist -- -p always"
  },
  "build": {
    "appId": "br.com.lucasklauck.unochat",
    "files": [
      "**/*",
      "dist/**/*"
    ],
    "directories": {
      "output": "release",
      "buildResources": "dist"
    },
    "asar": true,
    "mac": {
      "category": "br.com.lucasklauck.chat",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": "portable"
    },
    "linux": {
      "target": "deb"
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "my_new_project"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "mynewproject"
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.7",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "electron-compile": "^6.4.4",
    "electron-squirrel-startup": "^1.0.0",
    "primeflex": "^3.1.3",
    "primeicons": "^5.0.0",
    "primeng": "^13.3.3",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.2",
    "@angular/cli": "~13.2.2",
    "@angular/compiler-cli": "~13.2.0",
    "@apizee/apirtc": "^4.6.5",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "cli-truncate": "^3.1.0",
    "electron": "^18.0.3",
    "electron-builder": "^23.0.3",
    "electron-prebuilt-compile": "8.2.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
